{
  "VirtualDirectory": "/",
  "ConnectionStrings": {
    "DefaultConnection": "Persist Security Info=False;User ID=CommGateDBUser;password=CommGateDBUser;Initial Catalog=CommGateDB;Data Source=.;MultipleActiveResultSets=True;App=EntityFramework"
  },
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "Tokens": {
    "TokenExpireMinutes": 30,
    "RefreshTokenExpireMinutes": 2,
    "SocialNetworkTokenExpireMinutes": 2,
    "SecretKey": "System_secretkey#xeewx$",
    "Issuer": "CommGateClientIssuer",
    "Audience": "CommGateClientAudience",
    "TokenPath": "/api/token",
    "CookieName": "access_token",
    "Lifetime": 900
  },
  "EnableSwagger": true,
  "IdentityOptions": {
    "DefaultLockoutMinutes": "30",
    "MaxFailedAccessAttempts": "10"
  },
  "CorsPolicyConfiguration": [
    "http://localhost:4201"
  ],
  "AllowedHosts": "*"
}
